          function getDays(d, i) {
            if (d.getDate() === 1 || i === 0) {
              return \`\${formatDay(d)} \${formatMonth(d)}\`;
            }
            return formatDay(d);
          }

          function getDaily(d, i) {
            const last = scale.domain().length - 1;
            if (i === 0) {
              return \`\${formatDay(d)} \${formatMonth(d)}\`;
            }
            if (d.getDate() === 1) {
              return \`\${formatMonth(d)}\`;
            }
            if (d.getDay() === 5) {
              return \`\${formatDay(d)}\`;
            }
            if (i === last) {
