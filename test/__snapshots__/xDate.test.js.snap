              return format;
            },
            hours(d, i) {
              const format = getHours(d, i);
              return format;
            },
          } [intvl];

          function getHours(d, i) {
            if (d.getHours() === 1 || i === 0) {
              return \`\${formatHour(d)} \${formatDay(d)}\`;
            }
            return formatHour(d);
